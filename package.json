{
  "name": "pikchr-wasm",
  "description": "A fast and small port of Pikchr to WASM.",
  "version": "1.0.0",
  "main": "dist/speed.js",
  "types": "dist/speed.d.ts",
  "sideEffects": false,
  "scripts": {
    "clean": "rimraf dist",
    "compile": "tsc --skipLibCheck && tstei ./dist/size.js && tstei ./dist/speed.js",
    "compile:watch": "tsc --skipLibCheck --watch",
    "test": "node test/index.js",
    "update": "bash pikchr/update.sh",
    "prepublishOnly": "npm run clean && npm run compile && npm run test"
  },
  "bugs": {
    "url": "https://github.com/fabiospampinato/pikchr-wasm/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Fabio Spampinato",
    "email": "spampinabio@gmail.com"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fabiospampinato/pikchr-wasm.git"
  },
  "keywords": [
    "pic",
    "pikchr",
    "diagram",
    "diagrams",
    "fast",
    "small",
    "svg"
  ],
  "dependencies": {
    "once": "npm:@fabiospampinato/once@^1.0.0"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3",
    "typescript-transform-export-interop": "^1.0.4"
  }
}
